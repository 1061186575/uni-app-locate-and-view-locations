<view class="content"><block wx:if="{{!postData.password}}"><view><input style="height:35px;border:1px solid #4CD964;" type="text" data-event-opts="{{[['input',[['__set_model',['','inputPassword','$event',[]]]]]]}}" value="{{inputPassword}}" bindinput="__e"/><button type="primary" data-event-opts="{{[['tap',[['login',['$event']]]]]}}" bindtap="__e">login</button></view></block><block wx:else><view class="config"><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="control" bindtap="__e"><text class="{{[isShowConfig?'triangle-down':'triangle-right']}}"></text></view><block wx:if="{{isShowConfig}}"><view style="margin:10px;"><view>改变用户首页<input type="text" data-event-opts="{{[['input',[['__set_model',['','homePage','$event',[]]]]]]}}" value="{{homePage}}" bindinput="__e"/></view><view><checkbox-group data-event-opts="{{[['change',[['e1',['$event']]]]]}}" bindchange="__e"><label><checkbox value="true"></checkbox>过滤错误信息</label></checkbox-group></view><radio-group data-event-opts="{{[['change',[['radioChange',['$event']]]]]}}" bindchange="__e"><view><label class="radio"><radio value="recentPosition" checked="true"></radio>最近的信息</label><view class="_br"></view><label class="radio"><radio value="allPosition"></radio>所有的信息</label></view></radio-group><block wx:if="{{isShowInput}}"><view>用户名:<input class="input" type="text" data-event-opts="{{[['input',[['__set_model',['$0','userName','$event',[]],['postData']]]]]}}" value="{{postData.userName}}" bindinput="__e"/>日期:<input class="input" type="text" data-event-opts="{{[['input',[['__set_model',['$0','date','$event',[]],['postData']]]]]}}" value="{{postData.date}}" bindinput="__e"/>当前日期可选择的用户名:<picker value="{{userIndex}}" range="{{allUserName}}" data-event-opts="{{[['change',[['bindPickerChange',['$event']]]]]}}" bindchange="__e"><view class="uni-input input">{{allUserName[userIndex]}}</view></picker><button style="background:#009688;margin-top:5px;" type="primary" data-event-opts="{{[['tap',[['query',['$event']]]]]}}" bindtap="__e">查询</button></view></block></view></block></view></block><block wx:for="{{$root.l0}}" wx:for-item="d" wx:for-index="index" wx:key="index"><view><block wx:if="{{d.$orig.errMsg&&d.g0&&!isFilterErrPositon}}"><view class="item"><view>{{d.$orig.userName?'用户名: '+d.$orig.userName:''}}</view><view>{{"时间: "+d.$orig.time}}</view><view>{{"错误信息: "+d.$orig.errMsg}}</view></view></block><block wx:if="{{d.$orig.errMsg&&!d.g1}}"><view class="item"><view>{{d.$orig.userName?'用户名: '+d.$orig.userName:''}}</view><view>{{"时间: "+d.$orig.time}}</view><view>{{"国家: "+d.$orig.country}}</view><view>{{"省份: "+d.$orig.province}}</view><view>{{"城市: "+d.$orig.city}}</view><view>{{"区（县）: "+d.$orig.district}}</view><view>{{"街道信息: "+d.$orig.street}}</view><view>{{"街道门牌号: "+d.$orig.streetNum}}</view><view>{{"POI信息: "+d.$orig.poiName}}</view><view>{{"邮政编码: "+d.$orig.postalCode}}</view><view>{{"城市代码: "+d.$orig.cityCode}}</view><view>{{"纬度: "+d.$orig.latitude}}</view><view>{{"经度: "+d.$orig.longitude}}</view><view>{{"速度: "+d.$orig.speed+"m/s"}}</view><view>{{"位置的精确度: "+d.$orig.accuracy}}</view><view>{{"高度: "+d.$orig.altitude+"m"}}</view><view>{{"垂直精度: "+d.$orig.verticalAccuracy+"m"}}</view><view>{{"水平精度: "+d.$orig.horizontalAccuracy+"m"}}</view><button type="primary" data-event-opts="{{[['tap',[['openLocation',['$0','$1'],[[['positionInfo','',index,'latitude']],[['positionInfo','',index,'longitude']]]]]]]}}" bindtap="__e">从地图打开</button></view></block><block wx:if="{{!d.$orig.errMsg&&!isFilterErrPositon}}"><view class="item"><view>{{d.$orig.userName?'用户名: '+d.$orig.userName:''}}</view><view>{{"时间: "+d.$orig.time}}</view><view>错误信息: 没有errMsg!</view></view></block></view></block></view>